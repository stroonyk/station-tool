{
  "name": "station-tool-app",
  "version": "0.0.1",
  "description": "A website to contain the Station-Tool App",
  "main": "webpack.config.js",
  "scripts": {
    "build:app": "rspack build --config ./config/rspack.config.ts --mode production",
    "build:app-watch": "rspack build --config ./config/rspack.config.ts --analyze --watch --mode development",
    "build:prod": "npm-run-all clean:all copy:views -s build:app -s build:server",
    "build:server": "swc server -d dist/server --config-file ./config/.swcrc",
    "build:server-setup": "mkdir dist && mkdir dist/client && echo '{}' >> dist/client/assets-manifest.json",
    "build:server-watch": "swc server -d dist/server -w --config-file ./config/.swcrc",
    "clean:all": "rm -rf ./dist",
    "clean:app": "rm -rf ./dist/client",
    "clean:server": "rm -rf ./dist/server",
    "copy:views": "cpy './views' dist",
    "copy:views-watch": "nodemon --delay 2.5 -e pug -w ./views -x npm run copy:views",
    "dev": "npm-run-all clean:all -s build:server-setup --parallel copy:views-watch build:app-watch build:server-watch server:start-watch",
    "server:start": "node -r dotenv/config ./dist/server/index.js dotenv_config_path=./env/.env",
    "server:start-watch": "nodemon --delay 2.5 -e ts,js,json -w ./dist -x npm run server:start",
    "module:local": "npm uninstall @rradar/station-tool --f && npm install ../module --f",
    "module:published": "npm uninstall @rradar/station-tool --f && npm install @rradar/station-tool@latest --f"
  },
  "author": "The rradar Development Team",
  "contributors": [
    "Rapha Weedon"
  ],
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@rradar/accounts-module": "^10.2.10",
    "@rradar/accounts-sdk": "^10.3.2",
    "@rradar/core": "^2.1.25",
    "@rradar/dashboard-sdk": "^1.12.0",
    "@rradar/fontawesome": "^1.0.1",
    "@rradar/risk-tool": "file:../../risk-tool/module",
    "@rradar/station-tool": "file:../module",
    "@rradar/station-sdk": "^2.1.0",
    "@rspack/core": "^0.6.5",
    "@sentry/browser": "^8.12.0",
    "@sentry/node": "^6.5.1",
    "@swc/plugin-remove-console": "^2.0.8",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.20.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "js-cookie": "^2.2.1",
    "nanoid": "^3.3.6",
    "node-fetch": "^2.7.0",
    "pug": "^3.0.3",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "request-ip": "^3.3.0",
    "send": "^0.19.0",
    "serve-favicon": "^2.5.0",
    "slick-carousel": "^1.8.1",
    "trix": "^2.1.5"
  },
  "devDependencies": {
    "@rspack/cli": "^0.6.5",
    "@sentry/webpack-plugin": "^2.20.1",
    "@svgr/webpack": "^8.1.0",
    "@swc/cli": "0.1.x",
    "@swc/core": "1.3.x",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^20.7.0",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "assets-webpack-plugin": "^7.1.1",
    "autoprefixer": "^9.7.2",
    "cpy-cli": "^5.0.0",
    "cssnano": "^6.0.1",
    "narn": "^2.3.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "8.2.x",
    "postcss-cli": "8.3.x",
    "postcss-import": "14.0.x",
    "postcss-loader": "5.2.x",
    "process": "^0.11.10",
    "resolve-url-loader": "^4.0.0",
    "sass": "1.32.13",
    "sass-loader": "^10.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "overrides": {
    "trix": "$trix"
  }
}
